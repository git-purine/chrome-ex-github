{
  "manifest_version": 3,
  "name": "ChromeEx for Github",
  "short_name": "ChromeEx for Github",
  "version": "0.9.0.0",
  "description": "ChromeEx for Github",
  "action": {
    "default_title": "ChromeEx for Github"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://github.com/*/pull/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/pull/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}
